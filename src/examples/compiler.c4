let a = 6;    // 0110
let b = 3;    // 0011

let and_result = a & b;     // 0110 & 0011 = 0010 = 2
let or_result = a | b;      // 0110 | 0011 = 0111 = 7
let xor_result = a ^ b;     // 0110 ^ 0011 = 0101 = 5
let lshift = a << 1;        // 0110 << 1 = 1100 = 12
let rshift = a >> 1;        // 0110 >> 1 = 0011 = 3

return and_result + or_result + xor_result + lshift + rshift; // 2 + 7 + 5 + 12 + 3 = 29
